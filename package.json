{
  "title": "IDSS桌面应用",
  "version": "1.0.0",
  "description": "资产2.0",
  "main": "dist/main/index.cjs",
  "homepage": "https://www.idss-cn.com",
  "copyright": "copyright",
  "name": "electron-template",
  "author": "IDSS-FED",
  "private": true,
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "prebuild": "sh scripts/icon.sh",
    "build": "vue-tsc --noEmit --p packages/renderer/tsconfig.json && node scripts/build.mjs",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "init": "git config core.hooksPath .git/hooks/ && rm -rf .git/hooks && npx simple-git-hooks",
    "commit": "./node_modules/.bin/idss-git-commit",
    "verify-commit-message": "./node_modules/.bin/idss-git-verify-commit-message",
    "preinstall": "npx only-allow pnpm"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "@idss/git-commit-cli": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-vue": "^2.3.2",
    "commitizen": "^4.2.4",
    "electron": "18.2.0",
    "electron-builder": "^23.0.3",
    "element-plus": "^2.2.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "nano-staged": "^0.8.0",
    "normalize.css": "^8.0.1",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "simple-git-hooks": "^2.7.0",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.8",
    "vite-plugin-electron": "^0.4.4",
    "vue": "^3.2.33",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.34.11"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ]
}
